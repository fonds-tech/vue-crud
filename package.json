{
  "name": "vue-crud",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.20.0",
  "description": "vue-crud",
  "author": "fonds-tech",
  "license": "MIT",
  "homepage": "https://github.com/fonds-tech/vue-crud#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fonds-tech/vue-crud.git"
  },
  "bugs": "https://github.com/fonds-tech/vue-crud/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint",
    "prepublishOnly": "nr build",
    "release": "bumpp",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "element-plus": "catalog:",
    "lodash-es": "catalog:",
    "vue": "catalog:"
  },
  "devDependencies": {
    "@antfu/ni": "catalog:",
    "@antfu/utils": "catalog:",
    "@fonds/eslint-config": "catalog:",
    "@types/lodash-es": "catalog:",
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vitejs/plugin-vue-jsx": "catalog:",
    "bumpp": "catalog:",
    "eslint": "catalog:",
    "lint-staged": "catalog:",
    "sass": "catalog:",
    "simple-git-hooks": "catalog:",
    "tinyexec": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:",
    "vitest-package-exports": "catalog:",
    "vue-tsc": "catalog:",
    "yaml": "catalog:"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm i --frozen-lockfile --ignore-scripts --offline && npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
